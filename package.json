{
  "name": "cowlog",
  "version": "0.1.1",
  "description": "Development time logging for NodeJs developers",
  "homepage": "https://github.com/vidaxl-com/cowlog",
  "keywords": [
    "log",
    "logging",
    "tool",
    "tooling",
    "development"
  ],
  "bugs": {
    "url": "https://github.com/vidaxl-com/cowlog/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "test-dev": "node_modules/mocha/bin/mocha tests/tests && npm run clean",
    "test-dev-unit": "node_modules/mocha/bin/mocha tests/tests/unit && npm run clean",
    "test-dev-coverage": "node_modules/nyc/bin/nyc.js --reporter=lcov --reporter=clover --reporter=text node_modules/mocha/bin/mocha tests && npm run clean",
    "test": "npm run build && PROD=true node_modules/nyc/bin/nyc.js --reporter=lcov --reporter=clover --reporter=text node_modules/mocha/bin/mocha dist/tests/tests/**/*.js && npm run clean",
    "build": "npm run build-src && npm run build-test",
    "build-src": "mkdir tmp; rm -rf dist;mkdir dist;mkdir dist/src;node_modules/babel-cli/bin/babel.js src --presets babel-preset-env --out-dir dist/src",
    "build-test": "rm -rf test-build;mkdir dist/tests;node_modules/babel-cli/bin/babel.js tests --presets babel-preset-env --out-dir dist/tests",
    "docs-gen": "src/bin/update-docs.js",
    "clean": "rm -rf dist && rm -rf test-build",
    "prepublish": "npm run build-src && npm run build-test",
    "standard-fix": "node_modules/standard/bin/cmd.js --fix src/index.js src/**/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vidaxl-com/cowlog.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Imre Toth",
    "email": "tothmimre@gmail.com"
  },
  "contributors": {
    "name": "Panos Vakalopoulos",
    "email": "p.vakalopoulos@vidaxl.com"
  },
  "dependencies": {
    "array-unique": "^0.3.2",
    "bottlejs": "^1.7.0",
    "clone": "^2.1.1",
    "colors": "^1.1.2",
    "cowsay": "^1.1.9",
    "file-extension": "^4.0.2",
    "function-arguments": "^1.0.8",
    "is-array": "^1.0.1",
    "is-empty": "^1.2.0",
    "is-object": "^1.0.1",
    "is-string": "^1.0.4",
    "merge": "^1.2.0",
    "mime-types": "^2.1.18",
    "object-hash": "^1.2.0",
    "object-path": "^0.11.4",
    "sha256": "^0.2.0",
    "stacktrace-js": "^2.0.0",
    "stringify-object": "^3.2.0",
    "write": "^1.0.3"
  },
  "devDependencies": {
    "array-ify": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-typescript": "^7.0.0-alpha.19",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2 ",
    "codeclimate-test-reporter": "^0.5.0",
    "depcheck": "^0.6.8",
    "eslint-plugin-import": "^2.9.0",
    "fs-copy-file-sync": "^1.0.1",
    "glob": "^7.1.2",
    "kidnap-console": "0.0.2",
    "mocha": "5.0.0",
    "nyc": "^11.4.1",
    "recursive-readdir-sync": "^1.0.6",
    "standard": "^11.0.0"
  },
  "engines": {
    "node": "<=4.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "nyc": {
    "exclude": [
      "tests",
      "build"
    ]
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/coverage/",
      "/build/",
      "/tmp/"
    ],
    "globals": [
      "l",
      "lf"
    ]
  }
}
